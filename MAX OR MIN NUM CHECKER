#include<stdio.h>
int num1;
int num2;
int num3;
int num4;
int num5;
main()
{
	printf("Enter any number 1:");
	scanf("%d",&num1);
	printf("Enter any number 2:");
	scanf("%d",&num2);
	printf("Enter any number 3:");
	scanf("%d",&num3);
	printf("Enter any number 4:");
	scanf("%d",&num4);
	printf("Enter any number 5:");
	scanf("%d",&num5);
	
	{
	if(num1>num2 && num1>num3 && num1>num4 && num1>num5)
	{
		printf("%d is MAXIMUM",num1);
	}
	else if(num2>num1 && num2>num3 && num2>num4 && num2>num5)
	{
		printf("%d is MAXIMUM",num2);
	}
	else if(num3>num1 && num3>num2 && num3>num4 && num3>num5)
	{
		printf("%d is MAXIMUM",num3);
	}
	else if(num4>num1 && num4>num2 && num4>num3 && num4>num5)
	{
		printf("%d is MAXIMUM",num4);
	}
	else if(num5>num1 && num5>num2 && num5>num3 && num5>num4)
	{
		printf("%d is MAXIMUM",num5);
	}}
	
	
	if(num1<num2 && num1<num3 && num1<num4 && num1<num5)
	{
		printf("%d is MINIMUM",num1);
	}
	else if(num2<num1 && num2<num3 && num2<num4 && num2<num5)
	{
		printf("\n %d is MINIMUM\n",num2);
	}
	else if(num3<num1 && num3<num2 && num3<num4 && num3<num5)
	{
		printf("\n %d is MINIMUM\n",num3);
	}
	else if(num4<num1 && num4<num2 && num4<num3 && num4<num5)
	{
		printf("\n %d is MINIMUM\n",num4);
	}
	else if(num5<num1 && num5<num2 && num5<num3 && num5<num4)
	{
		printf("\n %d is MINIMUM\n",num5);
	}}
